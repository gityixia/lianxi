<div class="app-user-detail all">
	<div class="container-fluid all-content">
		<h1>Users Details</h1>
		<div class="row list text-right">
			<div class="col-lg-2">Username：</div>
			<div class="col-lg-10 text-left">{{data.username}}</div>
		</div>
		<div class="row list text-right">
			<div class="col-lg-2">Gender：</div>
			<div class="col-lg-10 text-left">{{data.sex}}</div>
		</div>
		<div class="row list text-right">
			<div class="col-lg-2">PhoneNum：</div>
			<div class="col-lg-10 text-left">{{data.telephone}}</div>
		</div>
		<div class="row list text-right">
			<div class="col-lg-2">Password：</div>
			<div class="col-lg-10 text-left">{{data.password}}</div>
		</div>
		<div class="row list text-right">
			<div class="col-lg-2">Introduction：</div>
			<div class="col-lg-10 text-left">{{data.info}}</div>
		</div>
	</div>
</div>